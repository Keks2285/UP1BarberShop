<Page x:Class="BarberShop.Stocker.SupplyDocumentViewer"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BarberShop.Stocker"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SupplyDocumentViewer">

    <Grid Background="Bisque">
        <FlowDocumentReader Background="White" Margin="50,0,50,0" IsScrollViewEnabled="True" ViewingMode="Scroll">
            <FlowDocument x:Name="SupplyDoc"  Background="white">
                <Paragraph >
                </Paragraph>
            </FlowDocument>
        </FlowDocumentReader>


    </Grid>
</Page>
