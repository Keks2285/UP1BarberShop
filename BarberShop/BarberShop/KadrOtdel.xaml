        <Label  Content="Серия паспорта" HorizontalAlignment="Center" Margin="44,53,130,0" VerticalAlignment="Top"/>
        <Label x:Name="Seria" Content="Label" HorizontalAlignment="Center" Margin="147,53,10,0" VerticalAlignment="Top" RenderTransformOrigin="0.474,1.192" Width="115"/>
        <Label  Content="Номер паспорта" HorizontalAlignment="Center" Margin="41,79,130,0" VerticalAlignment="Top"/>
        <Label x:Name="Nomer" Content="Label" HorizontalAlignment="Center" Margin="147,79,10,0" VerticalAlignment="Top" Width="115"/>
        <Label Content="Должности" HorizontalAlignment="Center" Margin="70,105,130,0" VerticalAlignment="Top"/>
        <Label x:Name="Posts" Content="Label" HorizontalAlignment="Center" Margin="147,105,10,0" VerticalAlignment="Top" Width="115"/>
