        <Button x:Name="Employeers" Content="Сотрудники" HorizontalAlignment="Center" Margin="147,193,50,0" VerticalAlignment="Top" Width="75" Click="Employeers_Click"/>
        <Button Content="Статусы" HorizontalAlignment="Center"  VerticalAlignment="Top" Width="75" Margin="178,218,29,0" Click="Button_Click"/>
        <Button Content="Назначение на должности" HorizontalAlignment="Center"  VerticalAlignment="Top" Width="163" Margin="10,218,109,0" Click="Button_Click"/>

